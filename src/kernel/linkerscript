OUTPUT_FORMAT("binary")

SECTIONS
{
	. = 0x00112000;

	.text :
	{
		_text_start = .;
		*(.text)
		_text_end = .;
	}

	.rodata :
	{
		_rodata_start = .;
		*(.rodata)
		_rodata_end = .;
	}

	.data :
	{
		_data_start = .;
		*(.data)
		_data_end = .;
        . = ALIGN(0x00116000);      /* カーネルのサイズを16KBにする */
	}

	.bss :
	{
		_bss_start = .;
		*(.bss)
		_bss_end = .;
	}

    /DISCARD/ : { *(.eh_frame) }
}
